<form name="form1" action="/users/login" method="post" id="form1">
    <div>
        <input type="hidden"  id="userPhone"/>
    </div>

    <div>
        <input type="hidden"  id="userPassword"/>
    </div>
    <span id="sppage" style="display: none"></span>
    <div class="small_yunduo">
    </div>
    <div class="login_box">
    </div>
    <div class="login_box logining_user">
        <input type="text" id="tbloginid" name="userPhone" class="input_style" required/><img src="/login/images/yiwen.png"
                                                                    class="yiwen" title="用户名默认为您接收短信的手机号！"/></div>
    <div class="login_box logining_pssword">
        <input type="password" id="tbpassword" name="userPassword" class="input_style" required/><img src="/login/images/yiwen.png"
                                                                         style="cursor: pointer" class="yiwen"
                                                                         title="<a href='/users/forgetpwd'>忘记密码?</a>"
                                                                         onclick='$(".findpassword").show("normal");'/>
    </div>
    <div class="login_box login_button_user" id="btnlogin">
    </div>
    <a href="/users/forgetPwd">
    <div class="login_box login_button_reset">
    </div>    </a>

