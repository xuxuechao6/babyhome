<script>
    $('#btnlogin').click(function () {
        var userPhone=$("#tbloginid").val();
        var userPassword=$("#tbpassword").val();
        if(userPhone==null||userPhone==""){
            /*top.art.*/alert( "请输入手机号！");
            $("#tbloginid").focus();
        } else if(userPassword==null||userPassword==""){
            /*top.art.*/alert( "请输入密码！");
            $("#tbpassword").focus();
        }else{
            $("#form1").submit();
        }
    });
</script>
